<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
	
        <id>WinSvcExecutor</id>
        <version>$version$</version>
        <title>Execute command line applications as Windows Services.</title>
        <authors>Jernej Rozac</authors>
		<copyright>Jernej Rozac</copyright>
        <owners />
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>This library makes it easy to create Windows Services for simple command line applications, which can run as a Windows Service or a command line application.</description>
		<projectUrl>https://github.com/jrozac/WinSvcExecutor</projectUrl>
		<licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
		
		<dependencies>
			<dependency id="CommandLineParser" version="2.2.1" />
			<dependency id="log4net" version="2.0.8" />
		</dependencies>
      
    </metadata>
    <files>
        <file src="bin\Release\WinSvcExecutor.dll" target="lib\net45\WinSvcExecutor.dll" />
        <file src="bin\Release\WinSvcExecutor.pdb" target="lib\net45\WinSvcExecutor.pdb" />
    </files>
</package>